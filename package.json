{
  "name": "cocoda-sdk",
  "version": "0.0.1",
  "description": "SDK for Cocoda",
  "main": "index.js",
  "scripts": {
    "test": "mocha --recursive",
    "lint": "eslint ./**/*.js ./**/.*.js",
    "fix": "eslint --fix ./**/*.js ./**/.*.js",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix"
