{
  "name": "cocoda-sdk",
  "version": "1.0.16",
  "description": "SDK for Cocoda",
  "main": "index.js",
  "jsdelivr": "dist/cocoda-sdk.js",
  "unpkg": "dist/cocoda-sdk.js",
  "browserslist": "defaults and not ie<=11 and supports es6",
  "files": [
    "dist",
    "lib",
    "errors",
    "utils",
    "providers"
  ],
  "scripts": {
    "test": "mocha --recursive",
    "lint": "eslint ./**/*.js ./**/.*.js",
    "fix": "eslint --fix ./**/*.js ./**/.*.js",
    "lint-staged": "lint-staged",
    "build": "rollup -c",
    "release": "npm run build && npm test && git checkout dev && git pull && npm version $SEMVER && git push && git checkout master && git merge dev && git push --follow-tags && git checkout dev",
    "release:patch": "SEMVER=patch npm run release",
    "release:minor": "SEMVER=minor npm run release",
    "release:major": "SEMVER=major npm run release"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix"
    ],
    "**/.*.js": [
      "eslint --fix"
    ]
  },
  "pre-commit": [
    "lint-staged",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gbv/cocoda-sdk.git"
  },
  "author": "Stefan Peters",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gbv/cocoda-sdk/issues"
  },
  "homepage": "https://github.com/gbv/cocoda-sdk#readme",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/helper-define-map": "^7.15.4",
    "@babel/preset-env": "^7.15.4",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "axios-mock-adapter": "^1.20.0",
    "eslint": "^7.32.0",
    "eslint-config-gbv": "^1.0.3",
    "lint-staged": "^11.1.2",
    "mocha": "^9.1.1",
    "mocha-eslint": "^7.0.0",
    "pre-commit": "^1.2.2",
    "rollup": "^2.56.3",
    "rollup-plugin-license": "^2.5.0"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "jskos-tools": "^1.0.17",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "uuid": "^8.3.2"
  }
}
