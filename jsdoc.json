{
  "source": {
    "include": ["./src/index.js", "./src/errors", "./src/providers", "./src/lib", "./src/utils"]
  },
  "plugins": [
    "plugins/markdown",
    "jsdoc-es6-fix.cjs"
  ],
  "markdown": {
    "idInHeadings": true
  },
  "templates": {
    "collapseSymbols": false,
    "outputSourceFiles": false
  },
  "opts": {
    "encoding": "utf8",
    "template": "node_modules/clean-jsdoc-theme",
    "readme": "README.md",
    "destination": "./out",
    "recurse": true
  }
}
